{
  "name": "mintlify-monorepo",
  "description": "Mintlify monorepo",
  "private": true,
  "author": "Mintlify, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint"
  },
  "bugs": {
    "url": "https://github.com/mintlify/mint/issues"
  },
  "license": "Elastic-2.0",
  "workspaces": {
    "packages": [
      "client",
      "packages/*",
      "apps/*"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build --filter=!@mintlify/client --filter=!@mintlify/dashboard --filter=!@mintlify/proxy --filter=!@mintlify/atlas",
    "build:vercel": "turbo run build --filter=!@mintlify/dashboard",
    "build:client": "turbo run build --filter=!@mintlify/dashboard --filter=!mint",
    "build:dashboard": "turbo run build --filter=!@mintlify/client --filter=!@mintlify/proxy",
    "build:docker": "turbo run build --filter=!@mintlify/atlas",
    "build:proxy": "turbo run build --filter=@mintlify/proxy",
    "build:storybook:client": "turbo run build:storybook --filter=@mintlify/client --",
    "clean:build": "turbo run clean:build && rimraf .turbo",
    "clean:all": "turbo run clean:all && rimraf .yarn/install-state.gz node_modules .turbo && echo Run `yarn` to reinstall dependencies",
    "test": "turbo run test",
    "type": "turbo run type",
    "format": "prettier \"./*.{json,md}\" --write && turbo run format",
    "format:check": "prettier \"./*.{json,md}\" --check && turbo run format:check",
    "lint": "turbo run lint",
    "lint:compare": "tsx --max-old-space-size=14336 --max-semi-space-size=512 scripts/lint-compare.mts",
    "depcheck": "tsx scripts/depcheck.mts",
    "cli-version": "tsx scripts/update-cli-version-map.mts",
    "update-stytch-types": "infisical run --env=local -- tsx scripts/update-stytch-types.mts"
  },
  "dependencies": {
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@sentry/nextjs": "^8.55.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.4.5",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@typescript-eslint/parser": "6.x",
    "@unleash/nextjs": "^1.6.2",
    "depcheck": "^1.4.5",
    "eslint": "8.x",
    "formidable": "^3.5.2",
    "husky": "8.0.3",
    "lerna": "9.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.1",
    "tsx": "^4.16.2",
    "turbo": "^2.5.8",
    "typescript": "^5.5.3"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@storybook/react-vite": "8.6.12",
    "eslint-plugin-storybook": "^0.12.0",
    "next": "15.5.9"
  },
  "resolutions": {
    "@babel/traverse": "7.23.2",
    "cipher-base": "^1.0.5",
    "form-data": "^4.0.2",
    "pbkdf2": "3.1.3",
    "next-cache-toolbar/next": "15.4.10",
    "@headlessui/react": "^2.2.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "import-in-the-middle": "1.14.2",
    "postcss": "8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-reconciler": "^0.32.0",
    "sharp": "^0.33.1",
    "shiki": "^3.17.0",
    "@asyncapi/specs": "6.8.1",
    "zod": "3.24.0",
    "elliptic": "6.6.1"
  }
}
